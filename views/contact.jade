extends layout

block content
  #fh5co-wrapper
  #fh5co-page
    header#fh5co-header-section.sticky-banner
      .container
        .nav-header
          a.js-fh5co-nav-toggle.fh5co-nav-toggle.dark(href='#')
            i
          h1#fh5co-logo
            a(href='/')
              i.icon-airplane
              | Travel
          // START #fh5co-menu-wrap
          nav#fh5co-menu-wrap(role='navigation')
            ul#fh5co-primary-menu.sf-menu
              li
                a(href='/') Home
              li
                a.fh5co-sub-ddown(href='vacation') Vacations
                ul.fh5co-sub-menu
                  li
                    a(href='#') Family
                  li
                    a(href='#') CSS3 &amp; HTML5
                  li
                    a(href='#') Angular JS
                  li
                    a(href='#') Node JS
                  li
                    a(href='#') Django &amp; Python
              li
                a(href='flight') Flights
              li
                a(href='hotel') Hotel
              li
                a(href='car') Car
              li
                a(href='blog') Blog
              li.active
                a(href='contact') Contact
    // end:header-top
    .fh5co-hero
      .fh5co-overlay
      .fh5co-cover(data-stellar-background-ratio='0.5' style='background-image: url(images/cover_bg_1.jpg);')
        .desc
          .container
            .row
              .col-sm-5.col-md-5
                .tabulation.animate-box
                  // Nav tabs
                  ul.nav.nav-tabs(role='tablist')
                    li(role='presentation')
                      a(href='#flights' aria-controls='flights' role='tab' data-toggle='tab') Flights
                    li.active(role='presentation')
                      a(href='#hotels' aria-controls='hotels' role='tab' data-toggle='tab') Hotels
                    li(role='presentation')
                      a(href='#packages' aria-controls='packages' role='tab' data-toggle='tab') Packages
                  // Tab panes
                  .tab-content
                    #flights.tab-pane(role='tabpanel')
                      .row
                        .col-xxs-12.col-xs-6.mt
                          .input-field
                            label(for='from') From:
                            input#from-place.form-control(type='text' placeholder='Los Angeles, USA')
                        .col-xxs-12.col-xs-6.mt
                          .input-field
                            label(for='from') To:
                            input#to-place.form-control(type='text' placeholder='Tokyo, Japan')
                        .col-xxs-12.col-xs-6.mt.alternate
                          .input-field
                            label(for='date-start') Check In:
                            input#date-start.form-control(type='text' placeholder='mm/dd/yyyy')
                        .col-xxs-12.col-xs-6.mt.alternate
                          .input-field
                            label(for='date-end') Check Out:
                            input#date-end.form-control(type='text' placeholder='mm/dd/yyyy')
                        .col-sm-12.mt
                          section
                            label(for='class') Class:
                            select.cs-select.cs-skin-border
                              option(value='' disabled='' selected='') Economy
                              option(value='economy') Economy
                              option(value='first') First
                              option(value='business') Business
                        .col-xxs-12.col-xs-6.mt
                          section
                            label(for='class') Adult:
                            select.cs-select.cs-skin-border
                              option(value='' disabled='' selected='') 1
                              option(value='1') 1
                              option(value='2') 2
                              option(value='3') 3
                              option(value='4') 4
                        .col-xxs-12.col-xs-6.mt
                          section
                            label(for='class') Children:
                            select.cs-select.cs-skin-border
                              option(value='' disabled='' selected='') 1
                              option(value='1') 1
                              option(value='2') 2
                              option(value='3') 3
                              option(value='4') 4
                        .col-xs-12
                          input.btn.btn-primary.btn-block(type='submit' value='Search Flight')
                    #hotels.tab-pane.active(role='tabpanel')
                      .row
                        .col-xxs-12.col-xs-12.mt
                          .input-field
                            label(for='from') City:
                            input#from-place.form-control(type='text' placeholder='Los Angeles, USA')
                        .col-xxs-12.col-xs-6.mt.alternate
                          .input-field
                            label(for='date-start') Return:
                            input#date-start.form-control(type='text' placeholder='mm/dd/yyyy')
                        .col-xxs-12.col-xs-6.mt.alternate
                          .input-field
                            label(for='date-end') Check Out:
                            input#date-end.form-control(type='text' placeholder='mm/dd/yyyy')
                        .col-sm-12.mt
                          section
                            label(for='class') Rooms:
                            select.cs-select.cs-skin-border
                              option(value='' disabled='' selected='') 1
                              option(value='economy') 1
                              option(value='first') 2
                              option(value='business') 3
                        .col-xxs-12.col-xs-6.mt
                          section
                            label(for='class') Adult:
                            select.cs-select.cs-skin-border
                              option(value='' disabled='' selected='') 1
                              option(value='1') 1
                              option(value='2') 2
                              option(value='3') 3
                              option(value='4') 4
                        .col-xxs-12.col-xs-6.mt
                          section
                            label(for='class') Children:
                            select.cs-select.cs-skin-border
                              option(value='' disabled='' selected='') 1
                              option(value='1') 1
                              option(value='2') 2
                              option(value='3') 3
                              option(value='4') 4
                        .col-xs-12
                          input.btn.btn-primary.btn-block(type='submit' value='Search Hotel')
                    #packages.tab-pane(role='tabpanel')
                      .row
                        .col-xxs-12.col-xs-6.mt
                          .input-field
                            label(for='from') City:
                            input#from-place.form-control(type='text' placeholder='Los Angeles, USA')
                        .col-xxs-12.col-xs-6.mt
                          .input-field
                            label(for='from') Destination:
                            input#to-place.form-control(type='text' placeholder='Tokyo, Japan')
                        .col-xxs-12.col-xs-6.mt.alternate
                          .input-field
                            label(for='date-start') Departs:
                            input#date-start.form-control(type='text' placeholder='mm/dd/yyyy')
                        .col-xxs-12.col-xs-6.mt.alternate
                          .input-field
                            label(for='date-end') Return:
                            input#date-end.form-control(type='text' placeholder='mm/dd/yyyy')
                        .col-sm-12.mt
                          section
                            label(for='class') Rooms:
                            select.cs-select.cs-skin-border
                              option(value='' disabled='' selected='') 1
                              option(value='economy') 1
                              option(value='first') 2
                              option(value='business') 3
                        .col-xxs-12.col-xs-6.mt
                          section
                            label(for='class') Adult:
                            select.cs-select.cs-skin-border
                              option(value='' disabled='' selected='') 1
                              option(value='1') 1
                              option(value='2') 2
                              option(value='3') 3
                              option(value='4') 4
                        .col-xxs-12.col-xs-6.mt
                          section
                            label(for='class') Children:
                            select.cs-select.cs-skin-border
                              option(value='' disabled='' selected='') 1
                              option(value='1') 1
                              option(value='2') 2
                              option(value='3') 3
                              option(value='4') 4
                        .col-xs-12
                          input.btn.btn-primary.btn-block(type='submit' value='Search Packages')
              .desc2.animate-box
                .col-sm-7.col-sm-push-1.col-md-7.col-md-push-1
                  p
                    | HandCrafted by 
                    a.fh5co-site-name(href='http://frehtml5.co/' target='_blank') FreeHTML5.co
                  h2 Exclusive Limited Time Offer
                  h3 Fly to Hong Kong via Los Angeles, USA
                  span.price $599
                  // <p><a class="btn btn-primary btn-lg" href="#">Get Started</a></p>
    #fh5co-contact.fh5co-section-gray
      .container
        .row
          .col-md-8.col-md-offset-2.text-center.heading-section.animate-box
            h3 Contact Information
            p
              | Far far away, behind the word mountains, far from the countries Vokalia and Consonantia, there live the blind texts.
        form(action='#')
          .row.animate-box
            .col-md-6
              h3.section-title Our Address
              p
                | Far far away, behind the word mountains, far from the countries Vokalia and Consonantia, there live the blind texts.
              ul.contact-info
                li
                  i.icon-location-pin
                  | 198 West 21th Street, Suite 721 New York NY 10016
                li
                  i.icon-phone2
                  | + 1235 2355 98
                li
                  i.icon-mail
                  a(href='#') info@yoursite.com
                li
                  i.icon-globe2
                  a(href='#') www.yoursite.com
            .col-md-6
              .row
                .col-md-6
                  .form-group
                    input.form-control(type='text' placeholder='Name')
                .col-md-6
                  .form-group
                    input.form-control(type='text' placeholder='Email')
                .col-md-12
                  .form-group
                    textarea.form-control(name='' id='' cols='30' rows='7' placeholder='Message')
                .col-md-12
                  .form-group
                    input.btn.btn-primary(type='submit' value='Send Message')
    #fh5co-testimonial(style='background-image:url(images/img_bg_1.jpg);')
      .container
        .row.animate-box
          .col-md-8.col-md-offset-2.text-center.fh5co-heading
            h2 Happy Clients
        .row
          .col-md-4
            .box-testimony.animate-box
              blockquote
                span.quote
                  span
                    i.icon-quotes-right
                p
                  | &ldquo;Far far away, behind the word mountains, far from the countries Vokalia and Consonantia, there live the blind texts. Separated they live in Bookmarksgrove right at the coast of the Semantics, a large language ocean.&rdquo;
              p.author
                | John Doe, CEO 
                a(href='http://freehtml5.co/' target='_blank') FREEHTML5.co
                span.subtext Creative Director
          .col-md-4
            .box-testimony.animate-box
              blockquote
                span.quote
                  span
                    i.icon-quotes-right
                p
                  | &ldquo;Far far away, behind the word mountains, far from the countries Vokalia and Consonantia, there live the blind texts.&rdquo;
              p.author
                | John Doe, CEO 
                a(href='http://freehtml5.co/' target='_blank') FREEHTML5.co
                span.subtext Creative Director
          .col-md-4
            .box-testimony.animate-box
              blockquote
                span.quote
                  span
                    i.icon-quotes-right
                p
                  | &ldquo;Far far away, behind the word mountains, far from the countries Vokalia and Consonantia, there live the blind texts. Separated they live in Bookmarksgrove right at the coast of the Semantics, a large language ocean.&rdquo;
              p.author
                | John Doe, Founder 
                a(href='#') FREEHTML5.co
                span.subtext Creative Director
    #map.fh5co-map
    
    include footer.jade
  include scripts.jade
