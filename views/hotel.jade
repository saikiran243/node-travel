                                option(value='' disabled='' selected='') 1
                                option(value='1') 1
                                option(value='2') 2
                                option(value='3') 3
                                option(value='4') 4
                          .col-xs-12
                            input.btn.btn-primary.btn-block(type='submit' value='Search Hotel')
                      #packages.tab-pane(role='tabpanel')
                        .row
                          .col-xxs-12.col-xs-6.mt
                            .input-field
                              label(for='from') City:
                              input#from-place.form-control(type='text' placeholder='Los Angeles, USA')
                          .col-xxs-12.col-xs-6.mt
                            .input-field
                              label(for='from') Destination:
                              input#to-place.form-control(type='text' placeholder='Tokyo, Japan')
                          .col-xxs-12.col-xs-6.mt.alternate
