                li
                  a(href='/') Home
                li
                  a.fh5co-sub-ddown(href='vacation') Vacations
                  ul.fh5co-sub-menu
                    li
                      a(href='#') Family
                    li
                      a(href='#') CSS3 &amp; HTML5
                    li
                      a(href='#') Angular JS
                    li
                      a(href='#') Node JS
                    li
